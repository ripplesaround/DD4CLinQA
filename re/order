python run_squad.py \
  --model_type bert \
  --model_name_or_path model/pytorch_model.bin \
  --do_eval \
  --do_lower_case \
  --train_file train-v1.1.json \
  --predict_file dev-v1.1.json \
  --per_gpu_train_batch_size 12 \
  --learning_rate 3e-5 \
  --num_train_epochs 2.0 \
  --max_seq_length 300 \
  --doc_stride 128 \
  --save_steps 5000 \
  --output_dir /re/bert-base-uncased_300/